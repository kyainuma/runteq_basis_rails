<%= search_form_for @q, url: url do |f| %>
  <div class="input-group mb-3">
    <%= f.search_field :first_name_or_last_name_cont, class: 'form-control mx-2', placeholder: t('defaults.search_word') %>
    <%= f.select :role_eq, [['一般', 0], ['管理者', 1]], {include_blank: '指定なし'}, class: 'form-control mx-2' %>
    <%= f.submit class: 'btn btn-primary input-group-append' %>
  </div>
<% end %>
